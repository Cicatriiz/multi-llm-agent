# Multi-LLM Agent Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Google AI API Key
GOOGLE_API_KEY=your_google_api_key_here

# DeepSeek API Key
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Optional: OpenRouter API Key (for additional model access)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Anthropic API Key (if you want to add Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Framework Configuration
MLA_LOG_LEVEL=INFO
MLA_CONFIG_PATH=config/default.yaml

# Cost and Rate Limiting
MLA_DAILY_COST_LIMIT=100.0
MLA_RATE_LIMIT_RPM=100

# Database Configuration (if using persistent storage)
DATABASE_URL=sqlite:///multi_llm_agent.db

# Redis Configuration (if using Redis for caching)
REDIS_URL=redis://localhost:6379/0
